{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "linterOptions": {
        "exclude": [
            "bin",
            "node_modules",
            "src/**/*.spec.ts"
        ]
    },
    "jsRules": {},
    "rules": {
        "quotemark": [
            true,
            "single"
        ],
        "no-console": [
            false
        ],
        "one-line": [
            true,
            "check-open-brace",
            "check-whitespace"
        ],
        "object-literal-sort-keys": [
            false
        ],
        "no-string-literal": false,
        "triple-equals": [
            false
        ]
    },
    "rulesDirectory": []
}
